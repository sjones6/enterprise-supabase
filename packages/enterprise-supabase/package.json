{
  "name": "enterprise-supabase",
  "description": "A starter kit for building enterprise applications with Supabase.",
  "author": {
    "name": "Spencer Jones",
    "email": "jones.spencera@gmail.com",
    "url": "https://github.com/sjones6"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sjones6/enterprise-supabase"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": ["src/**/*", "dist/**/*"],
  "version": "0.0.0",
  "scripts": {
    "dev": "npm run build -- --watch",
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "db:reset": "supabase db reset && npm run supabase:types",
    "supabase:start": "supabase start",
    "supabase:stop": "supabase stop",
    "supabase:types": "supabase gen types typescript --local --schema public > src/database.ts && cp src/database.ts ../../apps/example/src/lib/database.ts"
  },
  "peerDependencies": {
    "react": ">18.2.0",
    "react-query": "^3.39.3",
    "typescript": ">5.1.6"
  },
  "devDependencies": {
    "@supabase/supabase-js": "^2.31.0",
    "@types/react": "^18.2.18",
    "lint-staged": "^13.2.3",
    "react": "^18.2.0",
    "supabase": "^1.82.6",
    "typescript": "^5.1.6"
  },
  "dependencies": {
    "@supabase/auth-ui-react": "^0.4.2",
    "react-query": "^3.39.3"
  }
}
